{
  "name": "reactwebpack",
  "version": "1.0.0",
  "description": "Boilerplate for ReactJS project",
  "main": "index.js",
  "scripts": {
    "rimraf": "rimraf",
    "webpack": "webpack",
    "webpack-dev-server": "webpack-dev-server",
    "dll": "set NODE_ENV=production&& webpack --config config/webpack.dll.js --progress --profile --colors --display-error-details --bail",
    "predll": "npm run clean:dll",
    "dll:dev": "webpack --config config/webpack.dll.js --progress",
    "predll:dev": "npm run clean:dll:dev",
    "//server:dev": "webpack-dev-server --config config/webpack.dev.js --progress --profile --colors --watch --display-error-details --display-cached --content-base src/",
    "server:dev": "webpack-dev-server --config config/webpack.dev.js --progress --colors --display-error-details --display-cached",
    "clean": "npm cache clean && npm run rimraf -- node_modules doc coverage dist",
    "clean:dist": "npm run rimraf -- dist",
    "clean:dll": "npm run rimraf -- manifest/prod",
    "clean:dll:dev": "npm run rimraf -- manifest/dev",
    "preclean:install": "npm run clean",
    "clean:install": "npm set progress=false && npm install",
    "preclean:start": "npm run clean",
    "clean:start": "npm start",
    "server:prod": "http-server dist --cors",
    "build-prod": "npm run dll && npm run app-prod && npm run process-html",
    "build": "npm run build:dev",
    "prebuild:dev": "npm run clean:dist",
    "build:dev": "webpack --config config/webpack.dev.js --progress --profile --colors --display-error-details --display-cached",
    "prebuild:prod": "npm run clean:dist",
    "build:prod": "webpack --config config/webpack.prod.js  --progress --profile --colors --display-error-details --display-cached --bail",
    "//start": "npm run server:dev",
    "server": "node server.js",
    "start": "webpack-dev-server --config config/webpack.dev.js --progress --colors --display-error-details --display-cached"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "antd": "^1.6.5",
    "axios": "^0.13.1",
    "classnames": "^2.2.5",
    "echarts": "^3.2.2",
    "inversify": "^2.0.0-rc.1",
    "lodash": "^4.14.0",
    "mobx": "^2.4.0",
    "mobx-react": "^3.5.1",
    "react": "^15.2.1",
    "react-dom": "^15.2.1",
    "react-router": "^2.6.0",
    "reflect-metadata": "^0.1.3"
  },
  "devDependencies": {
    "assets-webpack-plugin": "^3.4.0",
    "autoprefixer": "^6.3.7",
    "babel-cli": "^6.11.4",
    "babel-core": "^6.11.4",
    "babel-eslint": "^6.1.2",
    "babel-loader": "^6.2.4",
    "babel-plugin-antd": "^0.4.1",
    "babel-plugin-transform-class-properties": "^6.11.5",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.11.6",
    "copy-webpack-plugin": "^3.0.1",
    "css-loader": "^0.23.1",
    "eslint": "^3.1.1",
    "eslint-config-airbnb": "^9.0.1",
    "eslint-loader": "^1.5.0",
    "eslint-plugin-import": "^1.12.0",
    "eslint-plugin-jsx-a11y": "^2.0.1",
    "eslint-plugin-react": "^5.2.2",
    "exports-loader": "^0.6.3",
    "expose-loader": "^0.7.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "gh-pages": "^0.11.0",
    "html-webpack-plugin": "^2.22.0",
    "imports-loader": "^0.6.5",
    "istanbul-instrumenter-loader": "^0.2.0",
    "json-loader": "^0.5.4",
    "less-loader": "^2.2.3",
    "mobx-react-devtools": "^4.2.3",
    "node-sass": "^3.8.0",
    "parse5": "^2.1.5",
    "postcss-loader": "^0.9.1",
    "precss": "^1.4.0",
    "raw-loader": "^0.5.1",
    "react-addons-test-utils": "^15.2.1",
    "react-hot-loader": "^3.0.0-beta.2",
    "rimraf": "^2.5.4",
    "sass-loader": "^4.0.0",
    "source-map-loader": "^0.1.5",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.1",
    "webpack-dev-middleware": "^1.6.1",
    "webpack-dev-server": "^1.14.1",
    "webpack-hot-middleware": "^2.12.2",
    "webpack-md5-hash": "^0.0.5",
    "webpack-merge": "^0.14.1"
  }
}
